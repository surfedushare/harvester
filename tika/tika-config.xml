<?xml version="1.0" encoding="UTF-8"?>
<properties>
    <parsers>
        <parser class="org.apache.tika.parser.DefaultParser"></parser>
    </parsers>
    <fetchers>
        <fetcher class="org.apache.tika.pipes.fetcher.http.HttpFetcher">
            <params>
                <name>http</name>
            </params>
        </fetcher>
    </fetchers>
    <server>
        <params>
            <enableUnsecureFeatures>true</enableUnsecureFeatures>
        </params>
    </server>
</properties>